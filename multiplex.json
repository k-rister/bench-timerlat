{
  "presets": {
  },
  "validations": {
    "integer_ge_zero": {
      "description": "a whole number >= 0",
      "args": [
          "dma-latency"
      ],
      "vals": "[0-9]*$"
    },
  
    "integer_gt_zero": {
      "description": "a whole number > 0",
      "args": [
	      "duration",
	      "period"
      ],
      "vals": "^[1-9][0-9]*$"
    },
    "integer_range": {
      "description": "a range of whole numbers >= 0",
      "args": [
	 "cpus",
     "housekeeping"
      ],
      "vals": "(\\d+(-\\d+)?(,\\s*\\d+(-\\d+)?)*|\\d+(-\\d+)?)"
    },
    "priority": {
      "description": "priority of the timerlat process",
      "args": [
	 "priority"
      ],
      "vals": "^[for]:([0-9]|[1-9][0-9])$"
    },
    "onoff": {
      "description": "on|off",
      "args": [
	 "smt"
      ],
      "vals": "on|off"
    }

  }
}

